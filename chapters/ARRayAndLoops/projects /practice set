     <script>
        // pushing 4 from the end and shifting 1 from the front to [5,6]
        let arr=[5,6];
        arr.push(4);
        console.log(arr);
        arr.unshift(1);
        console.log(arr);



        // making a method to get a particular number from an array
        function particular(){
            let i=0;
            for(i=0; i<arr.length; i++){
                if(i=2){
                    return arr[i];
                }
            }
        }

        let num = particular(arr);
        console.log(num);
        
        // creating an array copy using slice method
        let copy=[];
        copy= arr.slice(0,arr.length);
        console.log(copy);


        //concatinating the given  array strings using accumulator pattern
        let arr2 =['i am ', 'from ', 'rajipur ', 'kailali.'];
        let sum=[];
        // let [a,b,c,d] = arr2;
        // console.log(`${a}${b}${c}${d}`);

        for(let i=0; i<arr2.length; i++){
            sum = sum + arr2[i];
        }
        console.log(`${sum}`);
        
     </script>
